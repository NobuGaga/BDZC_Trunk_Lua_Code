-- 点燃烟花返回
function _P.OnIngniteFireBack(data)
	local actInfo = MActivity.GetFromId(data.actId)
	if actInfo ~= nil then
		for k,v in pairs(actInfo.fireworksList) do
			if v.index == data.index then	--已点燃
				v.isIgnite = 1
				break
			end
		end

		actInfo.leftFreeTimes = data.leftFreeTimes
		actInfo.igniteTimes = data.igniteTimes
		actInfo.leftBuyTimes = data.leftBuyTimes
	end

	if WindowMgr.IsCreated("UMidautumnFireworks") then
		WindowMgr.Call("UMidautumnFireworks", "OnIngniteFireBack", data)
	end

	_P.CheckFireworkRedPoint(data.actId)
end


