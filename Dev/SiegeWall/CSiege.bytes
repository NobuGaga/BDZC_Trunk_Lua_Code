-- S2C
function _P.OnInitData(data)
	print("OnInitData______________")
	print(data.activityID)
	print(data.phase)
	if data.stageData ~=nil then
		for k,v in pairs(data.stageData)do
			print("_stage__",v.stage)
			print("_point__",v.point)
			print("selfGetState___",v.selfGetState)
			print("selfGetState___",v.selfGetState)
			if v.itemList ~= nil then
				for k1,v1 in pairs(v.itemList)do
					print("_itemIndex____",v1.index)
					print("_goodid____",v1.goodid)
					print("_buyCount____",v1.buyCount)
				end
			end
		end
	end

	MSiege.SetAcitvityPhase(data.activityID,data.phase)
	MSiege.SetAllStageData(data.stageData)
	WindowMgr.Call("USiegeMain","SiegeInit")
end

function _P.OnGetStageMsg(data)
	print("________OnGetStageMsg_____________________")
	print("__phase__________",data.phase)
	if data.progress ~= nil then
		for k,v in pairs(data.progress)do
			print("progress_type_______",v.type)
			print("progress_progressCur_______",v.progressCur)
			print("progress_progressMax_______",v.progressMax)
		end
	end
	print("__lastUdisk__________",data.lastUdisk.ncur,data.lastUdisk.nmax)
	if data.buffs ~= nil then
		for k,v in pairs(data.buffs)do
			print("__buffs___________",v.key,v.value)
		end
	else
		print("没有buff！！！")
	end

	if data.enemy ~= nil then
		print("__enemy__________",data.enemy)
	end
	print("____________",data.staffs)
	print("____________",data.teams)
	
	------------------------------------------------------------------------------
	MSiege.SetSiegeStageMsg(data)
	WindowMgr.Call("USiegeMain","SiegeStageMsg")
end

