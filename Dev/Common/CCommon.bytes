--[[
	@parent: 挂载点(任何节点)
	@effectId: 特效表ID
	@posScaleField: 位置和缩放值参数表索引
]]
function _M.CreateActor(parent, prefabId, posScalefield, onLoadedCall, scaleArg)
	local prefab = JGuiManager.CreateActor(parent, tostring(prefabId))
	if onLoadedCall ~= nil then
		prefab:SetOnLoadedBeforeLoad(onLoadedCall)
	end
	local pos = nil
	local scale = nil
	if posScalefield ~= nil then
		local posScaleCsv = Csv.EffectIDPosScale[prefabId]
		if posScaleCsv ~= nil and posScaleCsv.field ~= nil then
			for k,v in pairs(posScaleCsv.field) do
				if v == posScalefield then
					local posInfo = posScaleCsv.pos[k]
					if posInfo ~= nil then
						pos = Vector2(posInfo[1], posInfo[2])
					end
					scale = posScaleCsv.scale[k]
					break
				end
			end
		end
		if scale == nil then
			local uiPosScaleCsv = Csv.EffectUIPosScale[posScalefield]
			if uiPosScaleCsv ~= nil then
				pos = Vector2(uiPosScaleCsv.pos[1], uiPosScaleCsv.pos[2])
				scale = uiPosScaleCsv.scale
			end
		end
	end
	_M.LoadPrefabEffect(prefab, prefabId, pos, nil, nil, scale, scaleArg)
	return prefab
end

function _M.CreateRole(parent, bodyPrefabId, headPrefabId, posScalefield, onLoadedCall, scaleArg)
	local isThatLoaded = false
	local body, head
	local func = function(actor)
		if isThatLoaded then
			body.Activity = true
			head.Activity = true
			
			local headGrap = head:SkeletonGetGraphic()
			if headGrap ~= nil then
				body:SkeletonFollowerGraphic(headGrap.gameObject, "head")
			end
			if onLoadedCall ~= nil then
				onLoadedCall(body, head)
			end
		else
			isThatLoaded = true

			actor.Activity = false
		end		
	end
	body = _M.CreateActor(parent, bodyPrefabId, posScalefield, function(actor)
		body = actor
		func(actor)
	end, scaleArg)
	head = _M.CreateActor(parent, headPrefabId, posScalefield, function(actor)
		head = actor
		func(actor)
	end, scaleArg)	
	return body, head
end

