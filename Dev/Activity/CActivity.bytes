function _P.DoUpdateFund(actType, data)
	local info = _P.StateInfo(data, actType)
	info.day = data.day or 0
	info.investEndTime = data.investEndTime

	if data.rewardList ~= nil and next(data.rewardList) ~= nil then
		info.rewardList = {}
		for k,v in pairs(data.rewardList) do
			info.rewardList[v.id] = v
		end
	end

	MActivity.UpdateAct(info.actId, actType, info)
	_P.CheckFundRedPoint(info.actId)

	if WindowMgr.IsCreated("UFundPanel") then
		WindowMgr.Call("UFundPanel", "UpdateViewData", info)
	end
end


function _P.CheckFundRedPoint(actId)
	local isShowRed = false

	local actInfo = MActivity.GetFromId(actId)
	if actInfo ~= nil and actInfo.rewardList ~= nil then
		local maxNum = math.min(actInfo.day, 7)
		for i=1, maxNum do
			if not actInfo.rewardList[i].isGeted then
				isShowRed = true
				break
			end
		end
	end

	CMain.UpdateRedpoint(isShowRed, Define.SysId.Fund, actId)
end


------------------------------------------中秋烟花--------------------------------------------------
function _P.DoUpdateFirework(actType, data)
	local info = _P.StateInfo(data, actType)
	info.leftFreeTimes = data.leftFreeTimes
	info.igniteTimes = data.igniteTimes
	info.leftBuyTimes = data.leftBuyTimes

	if data.fireworksList ~= nil and next(data.fireworksList) ~= nil then
		info.fireworksList = {}
		for k,v in pairs(data.fireworksList) do
			info.fireworksList[k] = v
		end
	end

	info.rewardedList = {}
	local fireworkConf = Csv.ActivityFireworks[info.actId]
	if fireworkConf ~= nil then
		for i=1,#fireworkConf.needNum do
			local isGeted = false

			if data.rewardedList ~= nil then
				for k,v in pairs(data.rewardedList) do
					if v == i then
						isGeted = true
						break
					end
				end
			end

			info.rewardedList[i] = {needNum = fireworkConf.needNum[i], isGeted = isGeted}
		end
	end
	info.title = fireworkConf.title

	MActivity.UpdateAct(info.actId, actType, info)
	_P.CheckFireworkRedPoint(info.actId)

	if WindowMgr.IsCreated("UMidautumnFireworks") then
		WindowMgr.Call("UMidautumnFireworks", "UpdateViewData", info)
	end
end


function _P.CheckFireworkRedPoint(actId)
	local isShowRed = false

	local actInfo = MActivity.GetFromId(actId)
	if actInfo ~= nil and actInfo.rewardedList ~= nil then
		local hasIgniteNum = actInfo.igniteTimes
		for k,v in pairs(actInfo.rewardedList) do
			if v.isGeted == false and hasIgniteNum >= v.needNum then
				isShowRed = true
				break
			end
		end
	end

	CMain.UpdateRedpoint(isShowRed, Define.SysId.MidAFestival, actId)
end


