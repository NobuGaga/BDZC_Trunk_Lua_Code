--奇遇完成个人任务返回
function _P.OnAdvPersonTask(data)
	local activityData = MActivity.GetFromId(data.actId)
	local oldCompanyLevel = activityData.companyLevel

	if activityData ~= nil then
		-- 刷新整个列表数据
		if data.personalTasklist ~= nil then
			activityData.personalTasklist = {}
			for k,v in pairs(data.personalTasklist) do
				local one = MActivity.GetNewOneTask(v)
				activityData.personalTasklist[one.taskIndex] = one
			end
		else
			local updateOne = activityData.personalTasklist[data.taskIndex]
			updateOne.taskState = data.state
		end

		if data.hotval then
			activityData.hotval = data.hotval
		end

		activityData.companyLevel = data.companyLevel
		activityData.exp = data.exp
		if data.todayLucky then
			activityData.todayLucky = data.todayLucky
		end
	end

	MActivity.AdventureBattleRewards = data.reward

	data.oldCompanyLevel = oldCompanyLevel
	if WindowMgr.IsCreated("UAdventureStaff") then
		WindowMgr.Call("UAdventureStaff","OnCompleteEvent", data, function( ... )
			if WindowMgr.IsCreated("UAdventureMain") then
				WindowMgr.Call("UAdventureMain", "UpdateOwnData", data, false)
			end
		end)
	end

	if WindowMgr.IsCreated("UAdventureBoxPanel") then
		WindowMgr.Call("UAdventureBoxPanel","OnCompleteEvent", data, function( ... )
			if WindowMgr.IsCreated("UAdventureMain") then
				WindowMgr.Call("UAdventureMain", "UpdateOwnData", data, false)
			end
		end)
	end

	if WindowMgr.IsCreated("UBattleBossNew") then
		MActivity.AdventureUpdateTmp = data
	end

	-- 刷新红点
	_P.CheckAdventureRedPoint(data.actId, Define.ActivityType.Adventure)
end

